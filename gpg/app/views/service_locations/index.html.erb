<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", service_locations_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_service_location_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> ServiceLocations</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.service_location.location_id", :default => t("activerecord.labels.location_id", :default => "Location_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @service_locations.each do |service_location| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= service_location.id %>
          </td>
                    <td>
            <%= link_to service_location.location_id, service_location_path(service_location) %>
          </td>
                    <td>
            <%= service_location.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", service_location_path(service_location) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_service_location_path(service_location) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", service_location_path(service_location), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
        <%= @service_locations  %>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
